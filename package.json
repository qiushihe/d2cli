{
  "name": "d2qdb",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm-run-all build:clean build:tsc build:tsc-alias",
    "build:clean": "rimraf dist",
    "build:tsc": "tsc --project tsconfig.json",
    "build:tsc-alias": "tsc-alias --project tsconfig.json",
    "test": "npm-run-all test:lint",
    "test:lint": "eslint --ext .js --ext .ts ./src ./test ./type",
    "test:lint:fix": "eslint --ext .js --ext .ts --fix ./src ./test ./type",
    "console": "node dist/src/cli/console.js"
  },
  "dependencies": {
    "base-x": "^4.0.0",
    "base64-js": "^1.5.1",
    "minimist": "^1.2.7",
    "node-fetch": "^2.6.7",
    "open-editor": "^4.0.0",
    "opener": "^1.5.2",
    "protocol-registry": "^1.3.16",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.17",
    "@types/node-fetch": "^2.6.2",
    "@types/opener": "^1.4.0",
    "@types/purdy": "^3.5.1",
    "@types/ramda": "^0.28.20",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
